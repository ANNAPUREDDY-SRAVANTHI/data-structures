#include<stdlib.h>
#include<stdio.h>
int top=-1,stk[20];
void push(char ch)
{
    top=top+1;
    stk[top]=ch;
}
void pop()
{
    top=top-1;
}
int main()
{
    char str[50];
    int i;
    printf("enter the string");
    gets(str);
    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]=='['||str[i]=='{'||str[i]=='(')
            push (str[i]);
        else if((str[i]==')' && stk[top]=='(')||
        (str[i]=='}' && stk[top]=='{')||
        (str[i]==']' && stk[top]=='['))
            pop();
        else
            continue;
    }
    if(top==-1)
    printf("balanced bracket");
    else
    printf("notÂ balanced");
}
